{
  "name": "@baz-scm/pgmq-ts",
  "version": "0.3.4",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "pnpm run lint && pnpm run format:fix",
    "lint": "eslint src --cache",
    "format:fix": "prettier --write \"**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx}\"",
    "test": "mocha --require ts-node/register test/**/*.spec.ts --exit"
  },
  "keywords": [
    "postgres",
    "postgresql",
    "message-queue",
    "queues",
    "postgresql-extension",
    "pgmq"
  ],
  "author": "Baz Technologies",
  "license": "MIT",
  "description": "Typescript wrapper equivalent for pgmq-rs",
  "dependencies": {
    "eslint-js": "github:eslint/js",
    "pg": "^8.13.0"
  },
  "repository": {
    "url": "https://github.com/baz-scm/pgmq-ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/chai": "^5.0.0",
    "@types/eslint": "^9.6.1",
    "@types/mocha": "^10.0.9",
    "@types/pg": "^8.11.10",
    "chai": "^5.1.1",
    "eslint": "^9.12.0",
    "mocha": "^11.7.0",
    "pgmq-js": "^1.3.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.34.1"
  },
  "pnpm": {
    "overrides": {
      "cross-spawn@>=7.0.0 <7.0.5": ">=7.0.5",
      "@eslint/plugin-kit@<0.2.3": ">=0.2.3",
      "esbuild@<=0.24.2": ">=0.25.0",
      "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
      "brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2",
      "@eslint/plugin-kit@<0.3.4": ">=0.3.4"
    }
  }
}
